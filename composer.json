{
    "name": "franken-php/console",
    "description": "A high-end TUI dashboard for Laravel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Franken Console Team",
            "email": "team@franken-php.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^9.0|^10.0|^11.0",
        "symfony/console": "^6.0",
        "react/event-loop": "^1.0",
        "react/stream": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.0",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Franken\\Console\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Franken\\Console\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Franken\\Console\\FrankenServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
